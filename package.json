{
  "name": "jhaystack",
  "version": "0.0.32",
  "description": "A JSON search utility",
  "main": "./bundle/bundle.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "clean": "rimraf bundle && rimraf lib",
    "build": "npm run build:lib && npm run build:wbp",
    "build:lib": "tsc",
    "build:wbp": "webpack --config webpack.config.js",
    "prepublish": "npm run clean && npm run build",
    "test": "jest --coverage --verbose"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fukurosan/Jhaystack.git"
  },
  "keywords": [
    "JSON",
    "search",
    "engine"
  ],
  "author": "Henrik Olofsson",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@types/jest": "^25.2.2",
    "babel-loader": "^8.1.0",
    "jest": "^24.9.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^25.5.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "typescript": "^3.9.2"
  }
