{
  "name": "jhaystack",
  "version": "0.0.41",
  "description": "JavaScript search engine",
  "main": "dist/bundle.cjs.min.js",
  "module": "dist/bundle.esm.js",
  "unpkg": "dist/bundle.umd.min.js",
  "jsdelivr": "dist/bundle.umd.min.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist && rimraf types && npm run lint:fix && npm run test && npm run build:types",
    "build": "rollup -c",
    "prettier": "prettier \"*/**/*.{js,ts}\" --write",
    "lint": "eslint \"*/**/*.{js,ts}\"",
    "lint:fix": "npm run prettier && eslint \"*/**/*.{js,ts}\" --fix",
    "test": "jest --verbose --coverage --no-cache",
    "test:watch": "jest --watch --verbose",
    "build:types": "tsc --declaration --emitDeclarationOnly --out ./dist/types.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fukurosan/Jhaystack.git"
  },
  "keywords": [
    "JavaScript",
    "search",
    "engine",
    "search-engine",
    "bitap",
    "typescript"
  ],
  "author": "Henrik Olofsson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fukurosan/Jhaystack/issues"
  },
  "homepage": "https://github.com/fukurosan/Jhaystack",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@types/jest": "^25.2.2",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "babel-jest": "^26.6.1",
    "core-js": "^3.6.5",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jest": "^24.1.0",
    "husky": "^4.3.0",
    "jest": "^24.9.0",
    "lint-staged": "^10.5.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.0",
    "rollup": "^2.32.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^25.5.1",
    "typescript": "^3.9.2"
  },
  "dependencies": {}
}
